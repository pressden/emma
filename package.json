{
  "name": "emma",
  "version": "1.0.21",
  "repository": "https://github.com/pressden/emma.git",
  "description": "Emma is a highly customizable parent theme powered by NPM, Webpack, SASS and ES6.",
  "main": "theme.js",
  "browserslist": [
    "> 2%",
    "ie > 9"
  ],
  "scripts": {
    "dev": "webpack --mode=development --watch --progress --colors --config webpack/dev.config.js",
    "watch": "webpack --mode=production --watch --progress --colors --hide-modules --config webpack/dev.config.js",
    "build": "webpack --mode=production --progress --hide-modules --config webpack/prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "D.S. Webster | Eric Michel",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.2.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {}
}
