{
  "name": "emma",
  "version": "1.0.21",
  "repository": "https://github.com/pressden/emma.git",
  "description": "Emma is a highly customizable parent theme powered by NPM, Webpack, SASS and ES6.",
  "main": "theme.js",
  "browserslist": [
    "> 2%",
    "ie > 9"
  ],
  "scripts": {
    "dev": "webpack --mode=development --watch --progress --colors --config webpack/dev.config.js",
    "watch": "webpack --mode=production --watch --progress --colors --hide-modules --config webpack/dev.config.js",
    "build": "webpack --mode=production --progress --hide-modules --config webpack/prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "D.S. Webster | Eric Michel",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "css-loader": "^3.6.0",
    "fibers": "^4.0.3",
    "file-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {}
}
