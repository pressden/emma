{
	"version": 2,
	"settings": {
		"color": {
			"link": true,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#0073aa",
					"name": "Primary",
					"contrast": "#fff"
				},
				{
					"slug": "primary-support",
					"color": "#d4f9f8",
					"name": "Primary Support"
				},
				{
					"slug": "secondary",
					"color": "#663f46",
					"name": "Secondary Support"
				},
				{
					"slug": "secondary-support",
					"color": "#b8d8d8",
					"name": "Secondary Support"
				},
				{
					"slug": "accent",
					"color": "#d34e24",
					"name": "Accent"
				},
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				},
				{
					"slug": "gray-200",
					"color": "#ebebeb",
					"name": "Gray 200"
				},
				{
					"slug": "gray-400",
					"color": "#d3d3d3",
					"name": "Gray 400"
				},
				{
					"slug": "gray-600",
					"color": "#737373",
					"name": "Gray 600"
				},
				{
					"slug": "gray-800",
					"color": "#393939",
					"name": "Gray 800"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				}
			]
		},
		"layout": {
			"contentSize": "782px",
			"wideSize": "1100px"
		},
		"typography": {
			"customLineHeight": true,
			"letterSpacing": false,
			"fontWeight": false,
			"fontStyle": false,
			"fluid": true,
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "1.25rem",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "clamp( 1.5rem, 1.4rem + 0.51vw, 1.75rem)",
					"name": "Medium",
					"fluid": {
						"min": "1.5rem",
						"max": "1.75rem"
					}
				},
				{
					"slug": "large",
					"size": "clamp( 1.7rem, 1.47rem + 1.13vw, 2.25rem)",
					"name": "Large"
				},
				{
					"slug": "huge",
					"size": "clamp( 2rem, 1.59rem + 2.05vw, 3rem)",
					"name": "Huge"
				},
				{
					"slug": "giant",
					"size": "clamp( 2.6rem, 1.82rem + 3.9vw, 4.5rem)",
					"name": "Giant"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "Sans Serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "Serif",
					"slug": "serif"
				}
			]
		},
		"spacing": {
			"customPadding": true,
			"customMargin": true,
			"blockGap": true,
			"units": [
				"rem"
			]
		},
		"border": {
			"customColor": true,
			"customRadius": true,
			"customStyle": true,
			"customWidth": true
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "2rem"
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily":"var(--wp--preset--font-family--sans-serif)",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				},
				"border": {
					"radius": 0
				},
				"spacing": {
					"padding": {
						"top": "0.5rem;",
						"right": "1rem",
						"bottom": "0.5rem",
						"left": "1rem"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"right": "0",
						"left": "0"
					}
				}
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--giant)",
					"fontWeight": "unset"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "unset"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "unset"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "unset"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "unset"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "unset"
				}
			}
		}
	}
}
